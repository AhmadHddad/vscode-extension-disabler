{
	"name": "extension-disabler",
	"displayName": "Extension disabler",
	"description": "will automatically disable extensions depending on workspace and profile.",
	"version": "0.1.2",
	"license": "MIT License",
	"publisher": "Ahmad-Na-Hddad",
	"preview": true,
	"icon": "images/logo.jpg",
	"homepage": "https://github.com/AhmadHddad/vscode-extension-disabler/blob/master/README.md",
	"author": {
		"name": "Ahmad Na Hddad",
		"url": "https://twitter.com/Ahmad_Na_Hddad"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/AhmadHddad/vscode-extension-disabler"
	},
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"vscode-settings",
		"profile",
		"extensions",
		"extension"
	],
	"activationEvents": [
		"onCommand:extension-disabler",
		"onCommand:extension-disabler-delete-profile",
		"onCommand:extension-disabler-edit-profile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Settings for the Extension Disabler",
			"properties": {
				"extensionDisabler.profiles": {
					"type": "array",
					"default": [],
					"description": "The names of the profiles that have been saved"
				},
				"extensionDisabler.storage": {
					"type": "object",
					"description": "These are the settings for each profile that has been saved. Probably don't hand-edit this",
					"patternProperties": {
						".*": {
							"type": "object"
						}
					}
				}
			}
		},
		"commands": [
			{
				"command": "extension-disabler",
				"title": "Extension disabler - Main"
			},
			{
				"command": "extension-disabler-delete-profile",
				"title": "Extension disabler - Delete Profile"
			},
			{
				"command": "extension-disabler-edit-profile",
				"title": "Extension disabler - Edit Profile"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run prepackage && npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"prepackage": "npm run linuxsqlitepackage && npm run macsqlitepackage && npm run winia32sqlitepackage && npm run winx64sqlitepackage",
		"linuxsqlitepackage": "./node_modules/.bin/node-pre-gyp install --directory=./node_modules/sqlite3 --target_platform=linux --target_arch=x64",
		"winia32sqlitepackage": "./node_modules/.bin/node-pre-gyp install --directory=./node_modules/sqlite3 --target_platform=win32 --target_arch=ia32",
		"winx64sqlitepackage": "./node_modules/.bin/node-pre-gyp install --directory=./node_modules/sqlite3 --target_platform=win32 --target_arch=x64",
		"macsqlitepackage": "./node_modules/.bin/node-pre-gyp install --directory=./node_modules/sqlite3 --target_platform=darwin --target_arch=x64"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/sqlite3": "^3.1.6",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"node-pre-gyp": "^0.17.0",
		"sqlite3": "^5.0.2"
	}
}
